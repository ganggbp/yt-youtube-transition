// prettier-ignore
{
  "extends": "@tsconfig/react-native/tsconfig.json",     /* Recommended React Native TSConfig base */
  "compilerOptions": {
    /* Visit https://aka.ms/tsconfig.json to read more about this file */

    /* Completeness */
    "skipLibCheck": true,                    
    "baseUrl":".",
    "paths": {
      "@assets": ["src/assets"],
      "@constants":["src/constants"],
      "@hooks":["src/hooks"],
      "@screens/*": ["src/screens/*"],
      "@components": ["src/components"],
      "@config":["src/config"],
      "@context/*":["src/context/*"],
      "src/*": ["src/*"],
      ".env": ["env"]
    },
  }
}
